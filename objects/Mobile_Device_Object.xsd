	<xs:import namespace="http://cybox.mitre.org/common-2" schemaLocation="../cybox_common.xsd"/>
	<xs:import namespace="http://cybox.mitre.org/objects#DeviceObject-2" schemaLocation="http://cybox.mitre.org/XMLSchema/objects/Device/2.0.1/Device_Object.xsd"/>
	<xs:element name="Mobile_Device" type="DeviceObj:DeviceObjectType">
		<xs:annotation>
			<xs:documentation>The Mobile_Device_Object object is intended to characterize a specific mobile device, such as a mobile phone or tablet.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="MobileDeviceObjectType">
		<xs:annotation>
			<xs:documentation>The MobileDeviceObjectType type is intended to characterize a specific mobile device, such as a mobile phone or tablet.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="DeviceObj:DeviceObjectType">
				<xs:sequence>
					<xs:element minOccurs="0" name="IMSI" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The IMSI field specifies the International Mobile Subscriber Identity (IMSI) value associated with the mobile system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="IMEI" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The IMEI field specifies the Internation Mobile Station Equipment Identity (IMSI) associated with the mobile device.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="ICCID" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The ICCID field specifies the Integrated Circuit Card Identifier (ICCID) value associated with the SIM card used by the mobile device.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="Firmware_Version" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The Firmware_Version field specifies the version of the firmware running on the mobile device.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
