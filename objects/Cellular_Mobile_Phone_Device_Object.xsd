	<xs:import namespace="http://cybox.mitre.org/common-2" schemaLocation="../cybox_common.xsd"/>
	<xs:import namespace="http://cybox.mitre.org/objects#CellularMobileDeviceObject-2" schemaLocation="Cellular_Mobile_Device_Object.xsd"/>
	<xs:element name="Mobile_Phone_Device" type="CellularMobilePhoneDeviceObj:CellularMobilePhoneDeviceObjectType">
		<xs:annotation>
			<xs:documentation>The Mobile_Phone_Device_Object object is intended to characterize a specific cellular mobile phone device.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="CellularMobilePhoneDeviceObjectType">
		<xs:annotation>
			<xs:documentation>The MobilePhoneDeviceObjectType type is intended to characterize a specific cellular mobile phone.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CellularMobileDeviceObj:CellularMobileDeviceObjectType">
				<xs:sequence>
					<xs:element minOccurs="0" name="Phone_Number" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The Phone_Number field specifies the phone number assigned to the mobile phone device.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
