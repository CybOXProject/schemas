	<xs:import namespace="http://cybox.mitre.org/common-2" schemaLocation="../cybox_common.xsd"/>
	<xs:import namespace="http://cybox.mitre.org/objects#SystemObject-2" schemaLocation="http://cybox.mitre.org/XMLSchema/objects/System/2.0.1/System_Object.xsd"/>
	<xs:element name="Mobile_System" type="SystemObj:SystemObjectType" nillable="true">
		<xs:annotation>
			<xs:documentation>The Mobile_System object is intended to characterize mobile computer systems, such as smartphones, as a combination of hardware and software.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="MobileSystemObjectType" mixed="false">
		<xs:annotation>
			<xs:documentation>The MobileSystemObjectType type is intended to characterize mobile computer systems, as a combination of hardware and software.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SystemObj:SystemObjectType">
				<xs:sequence>
					<xs:element minOccurs="0" name="IMSI" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The IMSI field specifies the International Mobile Subscriber Identity (IMSI) value associated with the mobile system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="IMEI" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The IMEI field specifies the Internation Mobile Station Equipment Identity (IMSI) associated with the mobile device.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="ICCID" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The ICCID field specifies the Integrated Circuit Card Identifier (ICCID) value associated with the SIM card used by the mobile device.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="Serial_Number" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The Serial_Number field captures the hardware serial number associated with the mobile device.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="Phone_Number" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The Phone_Number field specifies the phone number assigned to the mobile device.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="Firmware_Version" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The Firmware_Version field specifies the version of the firmware running on the mobile device.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
