	<xs:import namespace="http://cybox.mitre.org/common-2" schemaLocation="../cybox_common.xsd"/>
	<xs:element name="SMS_Message" type="SMSMessageObj:SMSMessageObjectType">
		<xs:annotation>
			<xs:documentation>The SMS_Message object is intended to characterize Short Message Service (SMS) messages.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="SMSMessageObjectType">
		<xs:annotation>
			<xs:documentation>The SMSMessageObjectType is intended to characterize Short Message Service (SMS) messages.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cyboxCommon:ObjectPropertiesType">
				<xs:sequence>
					<xs:element minOccurs="0" name="Sender" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The Sender field specifies the identity of the sender who sent the SMS message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="Subject" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The Subject field specifies the subject of the SMS message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="Body" type="cyboxCommon:StringObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The Body field specifies the body of the SMS message.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="Timestamp" type="cyboxCommon:DateTimeObjectPropertyType">
						<xs:annotation>
							<xs:documentation>The Timestamp element specifies the date/time at which the SMS message was sent.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
